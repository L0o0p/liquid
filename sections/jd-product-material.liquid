<style>
 @media only screen and (max-width:750px) {
    #shopify-section-{{ section.id }} .structureDescription_conter {
          
 } 
 {%- for block in section.blocks -%}


.structureList_text {
    margin-top: 10px;
}
 .structureList_textList {
    text-align: center;
 }
 .structureList_title {
    font-size: 16px;
    font-weight: 600;
    line-height: 1.5;
    color: #151515;
 }
.structureList_text {
    font-size: 12px;
    font-weight: 500;
    line-height: 1.5;
    color: #151515;
 }
 
 .structure_img {
    position: relative;
 }
 .structureList_spot_list {
    width: 100%;
    height: 100%;
 }
.structure_img img {
    width: 100%;
 }
 #block-{{ section.id }}-{{ block.id }}.structureList_spot1 {
    width: 20px;
    height: 20px;
    background: {{ block.settings.background }};
    border-radius: 50%;
    position: absolute;
    cursor: pointer;
      font-weight: 600;
    color: #FFFFFF;
    font-size: 14px;
    text-align: center;
    line-height: 1.5;
    display: flex;
    align-items: center;
    justify-content: center;
 }

 #block-{{ section.id }}-{{ block.id }}.structureList_spot1:after {
    content: '';
    position: absolute;
    width: 200%;
    height: 200%;
    border-radius: 50%;
    box-shadow: inset 0px 0px 10px 10px {{ block.settings.outside }};
    top: 50%;
    left: 50%;
    margin-top: -20px;
    margin-left: -20px;
    z-index: 1;
    opacity: 0;
    animation: halo 2s 1s infinite ease-out;
    }
    @-webkit-keyframes halo {
      0% {
        opacity: 0;
        -webkit-transform: scale(0.1);
      }

      50% {
        opacity: 1;
      }

      100% {
        opacity: 0;
        -webkit-transform: scale(1.2);
      }
    }
    .structure_img #block-{{ section.id }}-{{ block.id }}.structureList_spot1{
        {{ block.settings.m_location }}: {{ block.settings.m_range }}%;
        {{ block.settings.m_location1 }}: {{ block.settings.m_range1 }}%;
    }
    .structureList-textList1 .structureList_textList1:first-of-type {
        display: block;
      
    }
    .structureList-textList1 .structureList_textList1 {
        display: none;
       
    }
 .serialNumber {
      width: 25px;
    height: 25px;
    background: #000000;
    border-radius: 50%;
    text-align: center;
    font-weight: 600;
    color: #FFFFFF;
    line-height: 25px;
    font-size: 14px;
    margin-right: 13px;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 2px;
    }
    .structureList_leftText {
      width: 95%;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    }
    .structureList_active1 .serialNumber {
      background: #1274DD;
    }
     .structureList_active1 .structureList_title {
      color: #1274DD;
     }
     .structureList_serialNumber {
      display: flex;
    align-items: flex-start;
     }
     .structureList_box {
      text-align: left;
     }
    {%- endfor -%}
 }

@media only screen and (min-width:741px) {
    #shopify-section-{{ section.id }} .structureDescription_conter {
      margin-top: 10px;    
 } 
  #shopify-section-{{ section.id }} .structureList {
  display: flex;
    align-items: flex-start;
    justify-content: space-between;
 }
 {%- for block in section.blocks -%}
 .structureDescription-conter {
   
 }
.structure-main-img  {
  transition: all 0.5s;
  width: 100%;
    height: 100%;
    object-fit: cover;
}
.structureList_box {
    margin-top: 17px;
}
.structureList_box:first-child {
   margin-top: 0;
}

 .structureList_title {
   font-size: 20px;
    font-weight: 400;
    color: #000000;
    line-height: 30px;
 }
 .structureList_text p {
   font-size: 16px;
    font-weight: 400;
    color: #909090;
    line-height: 25px;
 }
 
 .structure_img {
    position: relative;
    width: 55%;
     padding-left: 54px;
    padding-right: 54px;
 }
 .structureList_spot_list {
    width: 100%;
    height: 100%;
 }

 #block-{{ section.id }}-{{ block.id }}.structureList_spot {
    width: 26px;
    height: 26px;
    background: {{ block.settings.background }};
    border-radius: 50%;
    position: absolute;
    cursor: pointer;
    font-weight: 600;
    color: #FFFFFF;
    font-size: 16px;
    text-align: center;
    line-height: 25px;
    display: flex;
    align-items: center;
    justify-content: center;
    
 }

 #block-{{ section.id }}-{{ block.id }}.structureList_spot:after {
    content: '';
    position: absolute;
    width: 200%;
    height: 200%;
    border-radius: 50%;
    box-shadow: inset 0px 0px 10px 10px {{ block.settings.outside }};
    top: 50%;
    left: 50%;
    margin-top: -26px;
    margin-left: -26px;
    z-index: 1;
    opacity: 0;
    animation: halo 2s 1s infinite ease-out;

    }
    @-webkit-keyframes halo {
      0% {
        opacity: 0;
        -webkit-transform: scale(0.1);
      }

      50% {
        opacity: 1;
      }

      100% {
        opacity: 0;
        -webkit-transform: scale(1.2);
      }
    }
    .structure_img #block-{{ section.id }}-{{ block.id }}.structureList_spot{
        {{ block.settings.location }}: {{ block.settings.range }}%;
        {{ block.settings.location1 }}: {{ block.settings.range1 }}%;
    }
    .structureList-textList {
      width: 40%;
      padding-top: 41px;
    }
     .structureList-textList .structureList_textList:first-child {
      margin-top: 0;
     }
    .structureList-textList .structureList_textList {
      text-align: left;
      margin-top: 18px;
      display: flex;
    align-items: flex-start;
    }
    .serialNumber {
      width: 26px;
    height: 26px;
    background: #000000;
    border-radius: 50%;
    text-align: center;
    font-weight: 600;
    color: #FFFFFF;
    line-height: 25px;
    font-size: 16px;
    margin-right: 13px;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 2px;
    }
    .structureList_leftText {
      width: 95%;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    }
    .structureList_active .serialNumber {
      background: #1274DD;
    }
     .structureList_active .structureList_title {
      color: #1274DD;
     }
    {%- endfor -%}
}
</style>
<div class="structureDescription_conter container">
   <div class="hidden-phone">
     <div class="structureDescription-conter">
       <div class="structureList">
         <div class="structure_img">
            <div class="structureList_spot_list">
                {% assign structureList = section.blocks | where: 'type', 'structureList' %}
                {% for block in structureList %}
                <div class="structureList_spot" aria-controls="block-{{ section.id }}-{{ block.id }}" id="block-{{ section.id }}-{{ block.id }}">{{ forloop.index }}</div>
                {% endfor %}
            </div>
            
            {%- if section.settings.structure_img -%}
            <img class="structure-main-img "  src="{{section.settings.structure_img | img_url: '820x'}}">
            {%- else -%}
            {{- 'image' | placeholder_svg_tag: 'structure__image placeholder-background hidden-phone' -}}
            {%- endif -%}

            {% for block in structureList %}
            {%- if block.settings.structureList_img -%}
            <img class="structure-main-img " style="display: none;"  src="{{block.settings.structureList_img | img_url: '820x'}}">
            {%- endif -%}
            {% endfor %}
            
         </div>
         <div class="structureList-textList">
            {% for block in section.blocks %}
            <div class="structureList_textList" aria-controls="block-{{ section.id }}-{{ block.id }}" id="block-{{ section.id }}-{{ block.id }}">
              <div class="serialNumber">{{ forloop.index }}</div>
              <div class="structureList_leftText">
                 {% if block.settings.structureList_title != blank or  block.settings.structureList_text != blank %}
                <div class="structureList_box">
                  {% if block.settings.structureList_title != blank %}
                   <div class="structureList_title">{{ block.settings.structureList_title }}</div>
               {% endif %}
                {% if block.settings.structureList_text != blank %}
                    <div class="structureList_text hidden-phone">{{ block.settings.structureList_text }}</div>
                <div class="structureList_text hidden-tablet-and-up">{{ block.settings.structureList_text | strip_html}}</div>
                {% endif %}
                </div>
               {% endif %}
             
                {% if block.settings.structureList_title1 != blank or block.settings.structureList_text1 != blank %}
                  <div class="structureList_box">
                   {% if block.settings.structureList_title1 != blank %}
                   <div class="structureList_title">{{ block.settings.structureList_title1 }}</div>
               {% endif %}
                {% if block.settings.structureList_text1 != blank %}
                    <div class="structureList_text hidden-phone">{{ block.settings.structureList_text1 }}</div>
                <div class="structureList_text hidden-tablet-and-up">{{ block.settings.structureList_text1 | strip_html}}</div>
                {% endif %}
               </div>
                {% endif %}
                {% if block.settings.structureList_title2 != blank or block.settings.structureList_text2 != blank %}
                  <div class="structureList_box">
               {% if block.settings.structureList_title2 != blank %}
                <div class="structureList_title">{{ block.settings.structureList_title2 }}</div>
               {% endif %}
                {% if block.settings.structureList_text2 != blank %}
                    <div class="structureList_text hidden-phone">{{ block.settings.structureList_text2 }}</div>
                <div class="structureList_text hidden-tablet-and-up">{{ block.settings.structureList_text2 | strip_html}}</div>
                {% endif %}
                  </div>
                {% endif %}
              </div>
            </div>
        {% endfor %}
         </div>
       </div>
    </div>
   </div>
   <div class="hidden-tablet-and-up">
     <div class="structureDescription-conter">
       <div class="structureList">
         <div class="structure_img">
            <div class="structureList_spot_list">
                {% assign structureList = section.blocks | where: 'type', 'structureList' %}
                {% for block in structureList %}
                <div class="structureList_spot1" aria-controls="block-{{ section.id }}-{{ block.id }}" id="block-{{ section.id }}-{{ block.id }}">{{ forloop.index }}</div>
                {% endfor %}
            </div>
            
            {%- if section.settings.structure_img -%}
            <img class="structure-mainImg "  src="{{section.settings.structure_img1 | img_url: '820x'}}">
            {%- else -%}
            {{- 'image' | placeholder_svg_tag: 'structure__image placeholder-background hidden-phone' -}}
            {%- endif -%}

            {% for block in structureList %}
            {%- if block.settings.structureList_img -%}
            <img class="structure-mainImg " style="display: none;"  src="{{block.settings.structureList_img | img_url: '820x'}}">
            {%- endif -%}
            {% endfor %}
            
         </div>
         <div class="structureList-textList1">
            {% for block in section.blocks %}
            <div class="structureList_textList1" aria-controls="block-{{ section.id }}-{{ block.id }}" id="block-{{ section.id }}-{{ block.id }}">
              <div class="structureList_serialNumber">
                 <div class="serialNumber">{{ forloop.index }}</div>
              <div class="structureList_leftText">
                 {% if block.settings.structureList_title != blank or  block.settings.structureList_text != blank %}
                <div class="structureList_box">
                  {% if block.settings.structureList_title != blank %}
                   <div class="structureList_title">{{ block.settings.structureList_title }}</div>
               {% endif %}
                {% if block.settings.structureList_text != blank %}
                    <div class="structureList_text hidden-phone">{{ block.settings.structureList_text }}</div>
                <div class="structureList_text hidden-tablet-and-up">{{ block.settings.structureList_text | strip_html}}</div>
                {% endif %}
                </div>
               {% endif %}
             
                {% if block.settings.structureList_title1 != blank or block.settings.structureList_text1 != blank %}
                  <div class="structureList_box">
                   {% if block.settings.structureList_title1 != blank %}
                   <div class="structureList_title">{{ block.settings.structureList_title1 }}</div>
               {% endif %}
                {% if block.settings.structureList_text1 != blank %}
                    <div class="structureList_text hidden-phone">{{ block.settings.structureList_text1 }}</div>
                <div class="structureList_text hidden-tablet-and-up">{{ block.settings.structureList_text1 | strip_html}}</div>
                {% endif %}
               </div>
                {% endif %}
                {% if block.settings.structureList_title2 != blank or block.settings.structureList_text2 != blank %}
                  <div class="structureList_box">
               {% if block.settings.structureList_title2 != blank %}
                <div class="structureList_title">{{ block.settings.structureList_title2 }}</div>
               {% endif %}
                {% if block.settings.structureList_text2 != blank %}
                    <div class="structureList_text hidden-phone">{{ block.settings.structureList_text2 }}</div>
                <div class="structureList_text hidden-tablet-and-up">{{ block.settings.structureList_text2 | strip_html}}</div>
                {% endif %}
                  </div>
                {% endif %}
              </div>
              </div>
            </div>
        {% endfor %}
         </div>
       </div>
    </div>
   </div>
</div>
<script>
     //点击定位点切换文字和图片
        $('.structureList_spot').click(function () {
            let index = $(this).index() + 1
            $(".structureList-textList .structureList_textList").eq($(this).index()).addClass('structureList_active').siblings().removeClass('structureList_active')
            $('.structure-main-img').eq(index).show().siblings('.structure-main-img').hide()
        })
          //点击定位点切换文字和图片
        $('.structureList_spot1').click(function () {
            let index = $(this).index() + 1
            $(".structureList-textList1 .structureList_textList1").eq($(this).index()).show().addClass('structureList_active1').siblings().hide().removeClass('structureList_active1')
            $('.structure-mainImg').eq(index).show().siblings('.structure-mainImg').hide()
        })
</script>
{% schema %}
{
"name": "Product Material",
"class": "productMaterial",
"settings": [
{
"type": "image_picker",
"id": "structure_img",
"label": "image"
},
{
"type": "image_picker",
"id": "structure_img1",
"label": "image m"
}
],
"blocks" : [
{
"type": "structureList",
"name": "structure_list",
"settings":[
{
"type": "image_picker",
"id": "structureList_img",
"label": "image"
},
{
"type": "text",
"id": "structureList_title",
"label": "title"
},
{
"type": "richtext",
"id": "structureList_text",
"label": "text"
},
{
"type": "text",
"id": "structureList_title1",
"label": "title"
},
{
"type": "richtext",
"id": "structureList_text1",
"label": "text"
},
{
"type": "text",
"id": "structureList_title2",
"label": "title"
},
{
"type": "richtext",
"id": "structureList_text2",
"label": "text"
},
{
"type": "header",
"content": "Colors"
},
{
