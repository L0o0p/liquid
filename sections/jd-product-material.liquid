},
{
"type": "select",
"id": "m_location1",
"label": "Mobile Start positioning from that position",
"options": [
{
"value": "left",
"label": "left"
},
{
"value": "right",
"label": "right"
}
],
"default": "left"
},
{
"type": "range",
"id": "m_range1",
"min": 0,
"max": 100,
"step": 1,
"unit": "%",
"label": "mobile positioning distance",
"default": 0
}
]
}
],
"presets": [{
"name": "Product Material"
}]
}
